{
  "name": "openvpn-status",
  "homepage": "https://github.com/AuspeXeu/openvpn-status",
  "description": "A web-based application to monitor clients connected to an openvpn server.",
  "version": "4.2.32",
  "license": "MIT",
  "main": "server.js",
  "author": {
    "name": "Christian Vaas",
    "email": "christianvaas@auspex.eu",
    "url": "https://auspex.eu"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run deps && npm run ncu"
    }
  },
  "keywords": [
    "OpenVPN",
    "monitoring",
    "vpn"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/AuspeXeu/openvpn-status.git"
  },
  "bugs": {
    "url": "https://github.com/AuspeXeu/openvpn-status"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deps": "depcheck . --ignores=\"husky,sqlite3,@vue/*,eslint-plugin-import,babel-eslint,vue-template-compiler\"",
    "ncu": "ncu"
  },
  "dependencies": {
    "axios": "1.7.7",
    "body-parser": "1.20.3",
    "express": "4.21.1",
    "maxmind": "4.3.22",
    "moment": "2.30.1",
    "nconf": "0.12.1",
    "netmask": "2.0.2",
    "sequelize": "6.37.5",
    "sqlite3": "5.1.7",
    "uuid": "11.0.2",
    "vue": "3.5.12",
    "vue-router": "4.4.5",
    "vuetify": "3.7.3",
    "vuex": "4.1.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/eslint-config-airbnb": "8.0.0",
    "babel-eslint": "10.1.0",
    "depcheck": "1.4.7",
    "eslint": "^8.2.0",
    "eslint-plugin-vue": "9.30.0",
    "husky": "9.1.6",
    "npm-check-updates": "17.1.9",
    "vue-template-compiler": "2.7.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "L": true,
      "ReconnectingWebSocket": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-mixed-operators": 0,
      "max-len": 0,
      "no-plusplus": 0,
      "no-return-assign": 0,
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "import/no-extraneous-dependencies": 0,
      "nonblock-statement-body-position": [
        "error",
        "below"
      ],
      "curly": [
        "error",
        "multi"
      ],
      "no-new-require": 0,
      "new-cap": 0,
      "semi": 0,
      "no-console": [
        "error",
        {
          "allow": [
            "log",
            "error"
          ]
        }
      ],
      "object-curly-spacing": 0,
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "_"
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "consistent-return": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "maintainers": [
    {
      "name": "Christian Vaas",
      "email": "chrisvaas@gmail.com"
    },
    {
      "name": "Estelle Beguin",
      "email": "beguin.stl@gmail.com"
    }
  ]
}
